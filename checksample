import random

count=0

while count<100000:
    count+=1
    mymega1  = random.sample (range (1,  70),  5)
    mymegaplier1  = random.sample (range(1, 25),1)
    mymega3 = sorted(mymega1)
    jackpot = [9,14,57,67,70]

    megaplier = [2]
    print("try: ",count)
    print("My Mega Million   ", mymega3,  "Megaball:", mymegaplier1)
    print("Real Mega Million ", jackpot, "Megaball:", megaplier)

    if (mymega3==jackpot )& (mymegaplier1==megaplier):
     break
